{
  "name": "kibo-nuxt-vue-starter",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi build",
    "start": "node -r dotenv/config .output/server/index.mjs",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".ts,.js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "prepare": "husky install",
    "prettier:check": "prettier --check .",
    "prettier:format": "prettier --write .",
    "lint": "yarn lint:js && yarn lint:style && yarn prettier:format",
    "test": "jest",
    "generate-types": "graphql-codegen --config codegen.yml",
    "prebuild": "node ./.build/preBuild.js",
    "predev": "yarn prebuild"
  },
  "lint-staged": {
    "*.{ts,js,vue,css}": [
      "yarn prettier --write"
    ],
    "*.{ts,js,vue}": [
      "yarn eslint"
    ],
    "*.{css,vue}": [
      "yarn stylelint"
    ],
    "*.{js}": [
      "yarn test"
    ]
  },
  "dependencies": {
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@kibocommerce/graphql-client": "^1.0.3-beta.7",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/style-resources": "^1.2.1",
    "@storefront-ui/nuxt": "^0.10.8",
    "@storefront-ui/vue": "^0.11.0",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "cookie-universal-nuxt": "^2.1.5",
    "core-js": "^3.15.1",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "graphql": "^16.0.1",
    "graphql-tag": "^2.12.5",
    "nuxt-edge": "latest",
    "nuxt-helmet": "^2.0.1",
    "nuxt-i18n": "^6.28.1",
    "stylelint-scss": "^3.21.0",
    "vue-i18n": "^8.26.1",
    "vue-lazy-hydration": "^2.0.0-beta.4",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "@babel/preset-typescript": "^7.15.0",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@graphql-codegen/cli": "2.3.0",
    "@graphql-codegen/typescript": "2.4.1",
    "@nuxt/bridge": "npm:@nuxt/bridge-edge",
    "@nuxt/types": "^2.15.7",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^8.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/fontawesome": "^1.1.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@types/jest": "^27.0.2",
    "@vue/test-utils": "^1.2.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^27.0.5",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^7.0.4",
    "jest": "^27.0.5",
    "lint-staged": "^12.1.2",
    "node-sass": "^6.0.1",
    "nodemon": "^2.0.13",
    "prettier": "^2.3.2",
    "sass": "^1.43.2",
    "sass-loader": "^10.1.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^22.0.0",
    "ts-jest": "^27.0.3",
    "vue-jest": "^3.0.4"
  }
}
